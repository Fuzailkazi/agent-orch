# Agent Workflows Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI API Key (required for LangChain agents)
OPENAI_API_KEY=sk-your-key-here

# MCP Server URL (optional, defaults to localhost:3000)
MCP_SERVER_URL=http://localhost:3000

# OpenAI Model (optional, defaults to gpt-4o)
# OPENAI_MODEL=gpt-4o

# Temperature for LLM responses (optional, defaults to 0)
# OPENAI_TEMPERATURE=0
